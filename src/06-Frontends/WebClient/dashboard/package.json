{
	"name": "dashboard",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "node --max_old_space_size=512 node_modules/@angular/cli/bin/ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"test-debug": "ng test --browsers Chrome_with_debugging",
		"test-headless": "ng test --watch=false --browsers=ChromeHeadless --reporters=progress,junit",
		"e2e": "ng e2e",
		"stats": "ng build --stats-json && webpack-bundle-analyzer ./dist/dashboard/stats.json",
		"stats-prod": "node --max_old_space_size=512 node_modules/@angular/cli/bin/ng build --prod --stats-json && webpack-bundle-analyzer ./dist/dashboard/stats.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~7.2.15",
		"@angular/common": "~7.2.15",
		"@angular/compiler": "~7.2.15",
		"@angular/core": "~7.2.15",
		"@angular/forms": "~7.2.15",
		"@angular/platform-browser": "~7.2.15",
		"@angular/platform-browser-dynamic": "~7.2.15",
		"@angular/router": "~7.2.15",
		"@fortawesome/angular-fontawesome": "^0.3.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.18",
		"@fortawesome/free-solid-svg-icons": "^5.8.2",
		"@ng-bootstrap/ng-bootstrap": "^4.1.2",
		"@ng-select/ng-select": "^2.19.0",
		"@ngx-loading-bar/core": "^4.2.0",
		"@ngx-loading-bar/http-client": "^4.2.0",
		"@ngx-loading-bar/router": "^4.2.0",
		"angular2-notifications": "^2.0.0",
		"bootstrap": "4.3.1",
		"cerialize": "^0.1.18",
		"core-js": "^2.6.5",
		"font-awesome": "^4.7.0",
		"lodash-es": "^4.17.11",
		"ngx-take-until-destroy": "^5.4.0",
		"rxjs": "~6.5.2",
		"tslib": "^1.9.3",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.13.9",
		"@angular/cli": "~7.3.9",
		"@angular/compiler-cli": "~7.2.15",
		"@angular/language-service": "~7.2.15",
		"@types/node": "~12.0.0",
		"@types/jasmine": "~3.3.12",
		"@types/jasminewd2": "~2.0.6",
		"codelyzer": "~5.0.1",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.5",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"protractor": "~5.4.2",
		"ts-node": "~8.1.0",
		"tslint": "~5.16.0",
		"tslint-lines-between-class-members": "^1.3.1",
		"typescript": "~3.2.4",
		"webpack-bundle-analyzer": "^3.3.2"
	}
}
