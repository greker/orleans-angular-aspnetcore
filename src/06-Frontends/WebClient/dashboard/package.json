{
	"name": "dashboard",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "node --max_old_space_size=512 node_modules/@angular/cli/bin/ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"test-debug": "ng test --browsers Chrome_with_debugging",
		"test-headless": "ng test --watch=false --browsers=ChromeHeadless --reporters=progress,junit",
		"e2e": "ng e2e",
		"stats": "ng build --stats-json && webpack-bundle-analyzer ./dist/dashboard/stats.json",
		"stats-prod": "node --max_old_space_size=512 node_modules/@angular/cli/bin/ng build --prod --stats-json && webpack-bundle-analyzer ./dist/dashboard/stats.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.1.3",
		"@angular/common": "~8.1.3",
		"@angular/compiler": "~8.1.3",
		"@angular/core": "~8.1.3",
		"@angular/forms": "~8.1.3",
		"@angular/platform-browser": "~8.1.3",
		"@angular/platform-browser-dynamic": "~8.1.3",
		"@angular/router": "~8.1.3",
		"@fortawesome/angular-fontawesome": "^0.4.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.19",
		"@fortawesome/free-solid-svg-icons": "^5.9.0",
		"@ng-bootstrap/ng-bootstrap": "^5.1.0",
		"@ng-select/ng-select": "^3.0.4",
		"@ngx-loading-bar/core": "^4.2.0",
		"@ngx-loading-bar/http-client": "^4.2.0",
		"@ngx-loading-bar/router": "^4.2.0",
		"angular2-notifications": "^2.0.0",
		"bootstrap": "4.3.1",
		"cerialize": "^0.1.18",
		"core-js": "^3.1.4",
		"font-awesome": "^4.7.0",
		"lodash-es": "^4.17.15",
		"ngx-take-until-destroy": "^5.4.0",
		"rxjs": "~6.5.2",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.801.2",
		"@angular/cli": "~8.1.2",
		"@angular/compiler-cli": "~8.1.3",
		"@angular/language-service": "~8.1.3",
		"@types/jasmine": "~3.3.16",
		"@types/jasminewd2": "~2.0.6",
		"@types/node": "~12.6.8",
		"codelyzer": "^5.1.0",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.2.0",
		"karma-chrome-launcher": "~3.0.0",
		"karma-coverage-istanbul-reporter": "~2.1.0",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"protractor": "~5.4.2",
		"rxjs-tslint-rules": "^4.24.3",
		"ts-node": "~8.3.0",
		"tslint": "~5.18.0",
		"tslint-lines-between-class-members": "^1.3.6",
		"typescript": "~3.4.5",
		"webpack-bundle-analyzer": "^3.3.2"
	}
}
